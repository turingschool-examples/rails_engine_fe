<div align="center">
<h1>Rails Engine</h1>
</div>

<div align="center" id="merchant_search">
  <%= form_with url: merchants_path, method: :get, local: true do |f| %>
  <%= f.hidden_field :query, value: "merchant_search" %>  
  <%= f.text_field :search, placeholder: "Search by merchant" %>
  <%= f.submit "Submit" %>
  <% end %>
</div>